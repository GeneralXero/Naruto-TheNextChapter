&FN_WHERE Helper-Object=$^\-?where$:[setq(0,[setunion(,lwho(%#))])][nspemit(%#,[ansi(+black,<)][ljust(,7)][ljust(Name,20)][ljust(Location,20)][ljust(Country,15)][rjust(Idle,10)][ansi(+black,>)][@@(End_Pemit)])][iter(%q0,[setq(Name,moniker(##))][setq(Location,loc(##))][setq(Idle,idle(##))][setq(isDark,hasflag(##,Dark))][nspemit(%#,[ansi(+black,<)][ljust([if(%q<IsDark>,ansi(+magenta,(Dark)))],7)][ljust(left(%q<Name>,20),20)][ljust(left(moniker(%q<Location>),20),20)][ljust([left(edit([if(isdbref([zone(%q<Location>)]),[moniker(zone(%q<Location>))],N/A)],Zone,),15)],15)][rjust(timestring(%q<idle>),10)][ansi(+black,>)][@@(End_Pemit)])][@@(End_Iter)],,%r)]
@set Helper-Object/FN_WHERE=regexp